    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bulma": "^0.9.2",
    "classnames": "^2.3.1",
    "lottie-web": "^5.7.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "react-waypoint": "^9.0.3",
